    <script>
        function success_opup(title,text) {
            Swal.fire({
                title: title,
                text: text,
                icon: "success",
                customClass: {
                    confirmButton: "btn btn-primary waves-effect waves-light"
                },
                buttonsStyling: !1
            })
        }

        function error_opup(title,text) {
            Swal.fire({
                title: title,
                text: text,
            icon: "error",
            customClass: {
                confirmButton: "btn btn-primary waves-effect waves-light"
            },
            buttonsStyling: !1
        })
        }

        

        $(document).ready(function () {
            var msgtype = "@TempData["MsgType"]";
            var  msg="@TempData["msg"]";
            if (msgtype == "Success") {
                success_opup("Success", msg)
            }

            if (msgtype == "Error") {
                error_opup("Error !", msg)
            }
        })
        $(document).ready(function () {
            $('.minus').click(function () {
                var $input = $(this).parent().find('input');
                var count = parseInt($input.val()) - 1;
                count = count < 1 ? 1 : count;
                $input.val(count);
                $input.change();
                return false;
            });
            $('.plus').click(function () {
                var $input = $(this).parent().find('input');
                $input.val(parseInt($input.val()) + 1);
                $input.change();
                return false;
            });
        });

        @RenderSection("scripts", required: false)
    </script>
   
