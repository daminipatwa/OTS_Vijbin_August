
@{
    ViewBag.Title = "Branch";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-xxl flex-grow-1 container-p-y">




    <div class="row">

        <!-- Multi  -->
        <div class="col-12">
            <div class="card">
                <h5 class="card-header">Branch Master</h5>
                <div class="card-body">

                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label>
                                    Add Branch
                                </label>
                                <input type="text" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <div class="row" style="margin-top: 5%;">
                        <div class="col-md-6">
                            <button type="button" class="btn btn-success btn-fab demo waves-effect waves-light">
                                <span class="tf-icons mdi mdi-checkbox-marked-circle-outline me-1"></span>Submit
                            </button>
                            <button type="button" class="btn btn-outline-secondary waves-effect">
                                <span class="tf-icons mdi mdi-close-box-outline me-1"></span>Reset
                            </button>
                        </div>

                    </div>

                </div>
            </div>
        </div>
        <!-- Multi  -->
    </div>
    <div class="row" style="margin-top:5%;">

        <!-- Multi  -->
        <div class="col-12">
            <div class="card">
                <h5 class="card-header">Branch List</h5>
                <div class="card-body">

                    <div class="row">
                        <div class="col-md-12">
                            <table class="datatables-basic table table-bordered">
                                <thead>
                                    <tr>
                                        <th>Id</th>
                                        <th>Name</th>
                                        <th>Created Date</th>
                                        <th>Created Userid</th>

                                        <th>Action</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>


                </div>
            </div>
        </div>
        <!-- Multi  -->
    </div>
</div>

