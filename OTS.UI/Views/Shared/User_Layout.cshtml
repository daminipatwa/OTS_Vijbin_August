    <script>
        $(document).ready(function () {
            var msgtype = "@TempData["MsgType"]";
            var msg = "@TempData["msg"]";
            if (msgtype == "Success") {
                success_opup("Success", msg)
            }

            if (msgtype == "Error") {
                error_opup("Error !", msg)
            }
        })
        function success_opup(title, text) {
            Swal.fire({
                title: title,
                text: text,
                icon: "success",
                customClass: {
                    confirmButton: "btn btn-primary waves-effect waves-light"
                },
                buttonsStyling: !1
            })
        }

        function error_opup(title, text) {
            Swal.fire({
                title: title,
                text: text,
                icon: "error",
                customClass: {
                    confirmButton: "btn btn-primary waves-effect waves-light"
                },
                buttonsStyling: !1
            })
        }

        function getnotification() {
            $.getJSON('../User/getnotification', function (data) {
                $("#spnncount").text(data.length);
                var html = "";
                for (var i = 0; i <= data.length - 1; i++) {
                    html += "<li class='list-group-item list-group-item-action dropdown-notifications-item'>";
                    html += "<div class='flex-grow-1'>";
                    html += "<a href='../User/AllOrder'><h6 class='small mb-1'></h6>";
                    html += "<small class='mb-1 d-block text-body'>"+data[i].msg+"</small> </div>";
                    html += "</div></a></li>";
                }
                $("#ul_not").append(html);
                $("#span_count_not").text(data.length);
                $("#span_not_count").text(data.length);
            })
        }

        $(document).ready(function () {
            getnotification();
        });
     @RenderSection("scripts", required: false)
    </script>
